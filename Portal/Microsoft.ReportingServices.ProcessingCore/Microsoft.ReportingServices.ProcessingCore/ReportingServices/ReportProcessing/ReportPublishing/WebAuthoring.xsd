    <xsd:complexType name="FieldSelectionType">
        <xsd:choice maxOccurs="unbounded">
            <xsd:element name="EntityName" type="xsd:string" minOccurs="1" maxOccurs="1" />
            <xsd:element name="PropertyName" type="xsd:string" minOccurs="1" maxOccurs="1" />
            <xsd:element name="HierarchyName" type="xsd:string" minOccurs="0" maxOccurs="1" />
            <xsd:element name="Aggregate" type="xsd:string" minOccurs="0" maxOccurs="1" />
            <xsd:element name="ParentPropertyName" type="xsd:string" minOccurs="0" maxOccurs="1" />
            <xsd:element name="HideTotal" type="xsd:boolean" minOccurs="0" maxOccurs="1" />
            <xsd:any namespace="##other" processContents="lax" />
        </xsd:choice>
        <xsd:anyAttribute namespace="##other" processContents="lax" />
    </xsd:complexType>
    <xsd:complexType name="FieldSelectionsType">
        <xsd:choice maxOccurs="unbounded">
            <xsd:element name="FieldSelection" type="FieldSelectionType" minOccurs="1" maxOccurs="unbounded" />
            <xsd:any namespace="##other" processContents="lax" />
        </xsd:choice>
        <xsd:anyAttribute namespace="##other" processContents="lax" />
    </xsd:complexType>
    <xsd:complexType name="FilterSelectionsType">
        <xsd:choice maxOccurs="unbounded">
            <xsd:element name="FilterSelection" type="FilterSelectionType" minOccurs="1" maxOccurs="unbounded" />
            <xsd:any namespace="##other" processContents="lax" />
        </xsd:choice>
        <xsd:anyAttribute namespace="##other" processContents="lax" />
    </xsd:complexType>
    <xsd:complexType name="FilterSelectionType">
        <xsd:choice maxOccurs="unbounded">
            <xsd:element name="DataSourceName" type="xsd:string" minOccurs="1" maxOccurs="1" />
            <xsd:element name="DatasetName" type="xsd:string" minOccurs="1" maxOccurs="1" />
            <xsd:element name="EntityName" type="xsd:string" minOccurs="1" maxOccurs="1" />
            <xsd:element name="PropertyName" type="xsd:string" minOccurs="1" maxOccurs="1" />
            <xsd:element name="ParentPropertyName" type="xsd:string" minOccurs="0" maxOccurs="1" />
            <xsd:element name="FilterType" type="xsd:string" minOccurs="1" maxOccurs="1" />
            <xsd:element name="FilterOperator" type="xsd:string" minOccurs="1" maxOccurs="1" />
            <xsd:element name="FilterValues" type="FilterValuesType" minOccurs="0" maxOccurs="unbounded" />
            <xsd:element name="AdvancedConditionValue" type="xsd:string" minOccurs="0" maxOccurs="1" />
            <xsd:element name="AdvancedConditions" type="AdvancedConditionsType" minOccurs="0" maxOccurs="unbounded" />
            <xsd:element name="ParameterName" type="xsd:string" minOccurs="0" maxOccurs="1" />
            <xsd:any namespace="##other" processContents="lax" />
        </xsd:choice>
        <xsd:anyAttribute namespace="##other" processContents="lax" />
    </xsd:complexType>
    <xsd:complexType name="FilterValuesType">
        <xsd:choice minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="FilterValue" type="FilterValueType" minOccurs="0" />
            <xsd:any namespace="##other" processContents="lax" />
        </xsd:choice>
    </xsd:complexType>
    <xsd:complexType name="FilterValueType">
        <xsd:choice minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="Label" type="xsd:string" minOccurs="1" maxOccurs="1" />
            <xsd:element name="FilterKeys" type="FilterKeysType" minOccurs="0" />
            <xsd:any namespace="##other" processContents="lax" />
        </xsd:choice>
    </xsd:complexType>
    <xsd:complexType name="FilterKeysType">
        <xsd:choice minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="FilterKey" type="FilterKeyType" minOccurs="0" />
            <xsd:any namespace="##other" processContents="lax" />
        </xsd:choice>
    </xsd:complexType>
    <xsd:complexType name="FilterKeyType">
        <xsd:choice minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="Property" type="xsd:string" minOccurs="0" />
            <xsd:element name="Value" type="xsd:string" minOccurs="0" />
            <xsd:any namespace="##other" processContents="lax" />
        </xsd:choice>
    </xsd:complexType>
    <xsd:complexType name="AdvancedConditionsType">
        <xsd:choice minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="AdvancedCondition" type="AdvancedConditionType" minOccurs="0" />
            <xsd:any namespace="##other" processContents="lax" />
        </xsd:choice>
    </xsd:complexType>
    <xsd:complexType name="AdvancedConditionType">
        <xsd:choice minOccurs="0" maxOccurs="unbounded">
            <xsd:element name="ConditionType" type="xsd:string" minOccurs="1" maxOccurs="1" />
            <xsd:element name="FilterOperator" type="xsd:string" minOccurs="1" maxOccurs="1" />
            <xsd:element name="AdvancedConditionValue" type="xsd:string" minOccurs="0" maxOccurs="1" />
            <xsd:any namespace="##other" processContents="lax" />
        </xsd:choice>
    </xsd:complexType>
    <xsd:simpleType name="DataTypeEnum">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Raw"/>
            <xsd:enumeration value="Summarized"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:element name="PreviewCommandText" type="xsd:string" />
    <xsd:element name="ThemeName" type="xsd:string" />
    <xsd:element name="StyleName" type="xsd:string" />
    <xsd:element name="WebAuthoringVersion" type="xsd:string" />
    <xsd:element name="FieldSelections" type="FieldSelectionsType" />
    <xsd:element name="DefaultView" type="xsd:string" />
    <xsd:element name="ShowGrandTotals" type="xsd:boolean" />
    <xsd:element name="PreviewDataType" type="DataTypeEnum" />
    <xsd:element name="FilterSelections" type="FilterSelectionsType" />
</xsd:schema>